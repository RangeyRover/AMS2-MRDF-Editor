<?xml version="1.0" encoding="UTF-8"?>
<mrdfDefinitions>

  <!-- ===================================================== -->
  <!-- ENUMERATIONS + BITMASKS -->
  <!-- ===================================================== -->
  <enums>

    <!-- Core enums used by multiple profiles -->
    <enum name="BOOL01">
      <value code="0">False</value>
      <value code="1">True</value>
    </enum>

    <enum name="ENGINE_TYPE">
      <value code="0x00">Don't use</value>
      <value code="0x01">V6</value>
      <value code="0x02">V8</value>
      <value code="0x03">V10</value>
      <value code="0x04">V12</value>
      <value code="0x05">Straight 4</value>
      <value code="0x06">Straight 5</value>
      <value code="0x07">Straight 6</value>
      <value code="0x08">Rotary 2</value>
      <value code="0x09">Rotary 3</value>
      <value code="0x0A">Flat 4</value>
      <value code="0x0B">Flat 6</value>
      <value code="0x0C">W16</value>
      <value code="0x0D">W12</value>
      <value code="0x0E">Single Cylinder</value>
      <value code="0x0F">Twin Cylinder</value>
      <value code="0x10">Flat 8</value>
      <value code="0x11">Flat 12</value>
    </enum>

    <enum name="DRIVETRAIN">
      <value code="0">RWD</value>
      <value code="1">AWD</value>
      <value code="2">FWD</value>
    </enum>

    <enum name="BOOST_TYPE">
      <value code="0">Natural Aspiration</value>
      <value code="1">Supercharged</value>
      <value code="2">Turbo</value>
    </enum>

    <enum name="ASPIRATION">
      <value code="0">Naturally aspirated</value>
      <value code="1">Boosted</value>
    </enum>

    <enum name="SHIFT_TYPE">
      <value code="0">H Pattern</value>
      <value code="1">Sequential</value>
    </enum>

    <enum name="TICK_RATE">
      <value code="180">180 Hz</value>
      <value code="360">360 Hz</value>
      <value code="540">540 Hz</value>
    </enum>

    <bitmask name="TYRE_BITS" baseType="uint8">
      <bit mask="0x01">Soft / Semi Slick</bit>
      <bit mask="0x02">Medium</bit>
      <bit mask="0x04">Hard</bit>
      <bit mask="0x08">Intermediate</bit>
      <bit mask="0x10">Wet</bit>
      <bit mask="0x20">Extreme</bit>
      <bit mask="0x40">All Weather</bit>
    </bitmask>

  </enums>

